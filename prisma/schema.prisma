// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Employee {
  id           Int            @id @default(autoincrement())
  name         String
  last_name    String
  email        String
  phone        String
  address      String
  picture      String
  experience   Experience[]
  skill        Skill[]
  wish         Wish[]
  education    Education[]
  other        Other[]
  company_info Company_info[]

}

model Experience {
  id       Int        @id @default(autoincrement())
  clients  String
  industry String
  network  String
  employee Employee[]
}

model Skill {
  id           Int        @id @default(autoincrement())
  software     String
  languages    String
  professional String
  softskill    String
  employee     Employee[]
}

model Wish {
  id                Int        @id @default(autoincrement())
  project           String
  industry          String
  further_education String
  employee          Employee[]
}

model Education {
  id       Int        @id @default(autoincrement())
  degree   String
  employee Employee[]
}

//
model Other {
  id             Int        @id @default(autoincrement())
  hobbies        String
  special_skills String
  employee       Employee[]
}

//
model Company_info {
  id         Int        @id @default(autoincrement())
  location   String
  department String
  position   String
  employee   Employee[]
}
